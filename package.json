{
  "name": "@karafra/nestjs-amqp",
  "version": "1.1.0",
  "description": "Minimal AMQP client for NestJS",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "patch": "npm run build && npm version patch && npm publish",
    "format": "prettier --write 'src/**/*.ts'",
    "build": "tsc",
    "prepublish": "npm run build",
    "generate-doc": "typedoc",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karafra/nestjs-amqp"
  },
  "bugs": {
    "url": "https://github.com/karafra/nestjs-amqp/issues"
  },
  "contributors": [
    {
      "name": "Karafra",
      "email": "dariusKralovic@protonamil.com"
    }
  ],
  "keywords": [
    "karafra",
    "npm",
    "typescript",
    "NestJS",
    "amqp"
  ],
  "author": "Karafra<dariusKralovic@protonamil.com",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "16.2.1",
    "@commitlint/config-conventional": "16.2.1",
    "@types/jest": "27.4.1",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.5",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.1.3",
    "ts-node": "10.7.0",
    "typedoc": "0.22.13",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@cloudamqp/amqp-client": "^2.0.3",
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "lint-staged": {
    "*.{ts}": "prettier --write"
  }
}
